<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.costaneto.anewworld">

    <application
        android:name="com.costaneto.anewworld.InteractiveStoryApplication"
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ANewWorld">
        <activity
            android:name="com.costaneto.anewworld.ui.StoryActivity"
            android:exported="true"
            android:parentActivityName="com.costaneto.anewworld.ui.MainActivity"/>
        <activity
            android:name="com.costaneto.anewworld.ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>